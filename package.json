{
  "name": "google-apps-script-utils",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "clasp": "clasp",
    "create": "clasp create",
    "push": "clasp push",
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss,sass}\" --ignore-path .prettierignore",
    "deploy": "echo $CLASPCONF_JSON >| .clasp.json && yarn build && clasp push -f",
    "json": "echo $CLASPCONF_JSON >| .clasp.json"
  },
  "devDependencies": {
    "@google/clasp": "^2.4.2",
    "prettier": "^2.5.1"
  }
}
